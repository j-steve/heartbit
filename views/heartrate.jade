extends layout

block content
	style.
		label input {display:block;}
		
	form.content(method="POST")
		h3= "Heartrate"
		label
			span="From"
			input(type="date", name="dateStart", value=dateStart)
		label
			span="To"
			input(type="date", name="dateEnd", value=dateEnd)
		input(type="submit", value="Go")
		pre= heartrateData
				